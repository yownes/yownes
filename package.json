  "main": "index.js",
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "turbo": "^1.1.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "turbo run build --parallel",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "clean": "turbo run clean && rm -rf node_modules",
    "change": "changeset add",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "private": true,
